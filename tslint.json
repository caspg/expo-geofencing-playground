{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended",
      "tslint-react"
  ],
  "jsRules": {},
  "rules": {
      "quotemark": [true, "single", "jsx-double"],
      "semicolon": [true, "never"],
      "object-literal-sort-keys": false,
      "member-access": false,
      "jsx-boolean-value": [true, "never"],
      "jsx-no-multiline-js": false,
      "interface-name" : [true, "never-prefix"],
      "jsx-no-lambda": false
  },
  "rulesDirectory": []
}
